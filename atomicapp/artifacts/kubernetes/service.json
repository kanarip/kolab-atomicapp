{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "kolab",
        "labels": {
            "name": "kolab"
        }
    },
    "spec": {
        "ports": [
            {
                "name": "manticore",
                "protocol": "TCP",
                "port": 8080,
                "targetPort": 8080,
                "nodePort": 0
            },
            {
                "name": "mongodb",
                "protocol": "TCP",
                "port": 27017,
                "targetPort": 27017,
                "nodePort": 0
            }
        ],
        "selector": {
            "name": "kolab"
        }
    },
    "status": {
        "loadBalancer": {}
    }
}
