{
    "kind": "ReplicationController",
    "apiVersion": "v1",
    "metadata": {
        "name": "kolab-asav-out",
        "labels": {
            "name": "kolab-asav-out"
        }
    },
    "spec": {
        "replicas": 1,
        "selector": {
            "name": "kolab-asav-out"
        },
        "template": {
            "metadata": {
                "labels": {
                    "name": "kolab-asav-out"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "kolab-asav-out",
                        "image": "kolab/asav-out",
                        "ports": [
                            {
                                "containerPort": 10024
                            }
                        ]
                    }
                ]
            }
        }
    }
}
