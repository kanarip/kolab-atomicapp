{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "kolab-webclient",
        "labels": {
            "name": "kolab-webclient"
        }
    },
    "spec": {
        "ports": [
            {
                "name": "http",
                "port": 80,
                "targetPort": 80
            },
            {
                "name": "https",
                "port": 443,
                "targetPort": 443
            }
        ],
        "selector": {
            "name": "kolab-webclient"
        },
        "type": "LoadBalancer"
    }
}
